generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model user {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  fname    String
  lname    String
  username String
}

model auth {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  uid      String @unique
  password String
}

model admin {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  uid String @unique
}
